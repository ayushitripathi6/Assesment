@model IEnumerable<WebAppAssesment.Models.Student>

@{
    ViewBag.Title = "Display";
    
}

<h2 class="bg-primary">Display</h2>
<h2 class="bg-info text-white">Total Student:@ViewBag.Total</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table bg-success text-white" >
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.SId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SDOB)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SContact)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.SId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SDOB)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SContact)
        </td>
      
    </tr>
}

</table>
